                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:beginLifespanVersion</targetAttribute>
                    <encodeIfEmpty>true</encodeIfEmpty>
                    <sourceExpression>
