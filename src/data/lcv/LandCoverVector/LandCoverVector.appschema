                    <sourceExpression>
                        <OCQL>'http://www.keskkonnaagentuur.ee/et/eesmargid-tegevused/projektid/corine-land-cover'</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcn:externalDescription/base2:DocumentCitation/base2:name</targetAttribute>
                    <sourceExpression>
