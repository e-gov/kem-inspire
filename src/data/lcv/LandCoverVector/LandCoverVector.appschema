                    <sourceExpression>
                        <OCQL>'Keskkonnaagentuur'</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcn:responsibleParty/base2:RelatedParty/base2:contact/base2:Contact/base2:electronicMailAddress</targetAttribute>
                    <sourceExpression>
