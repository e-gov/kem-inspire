                    <value>spatialite</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>${inspire.lcv}</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>http://inspire.ec.europa.eu/schemas/lcv/4.0/LandCoverVector.xsd</schemaUri>
        </FeatureType>
