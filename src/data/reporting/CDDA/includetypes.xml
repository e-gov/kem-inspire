<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<appschema:AppSchemaDataAccess xmlns:ns2="http://www.opengis.net/ogc" xmlns:appschema="http://www.geotools.org/app-schema">
    <namespaces>
        <Namespace>
            <prefix>xsi</prefix>
            <uri>http://www.w3.org/2001/XMLSchema-instance</uri>
        </Namespace>
        <Namespace>
            <prefix>xlink</prefix>
            <uri>http://www.w3.org/1999/xlink</uri>
        </Namespace>
        <Namespace>
            <prefix>cdda</prefix>
            <uri>https://eionet.europa.eu/reporting/cdda/draft</uri>
        </Namespace>
    </namespaces>
    <includedTypes/>
    <sourceDataStores>
        <DataStore>
            <id>dataStore</id>
            <parameters>
                <Parameter>
                    <name>dbtype</name>
                    <value>spatialite</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>${inspire.ps}</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>https://eionet.europa.eu/reporting/cdda/draft/CDDA_2018_Type2.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <mappingName>designatedarea</mappingName>
            <sourceDataStore>dataStore</sourceDataStore>
            <sourceType>cdda_reporting_designatedarea</sourceType>
            <targetElement>cdda:DesignatedArea</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>FEATURE_LINK</targetAttribute>
                    <idExpression>
                        <OCQL>pslocalid</OCQL>
                    </idExpression>
                    <sourceExpression>
                        <OCQL>cdda_reporting</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>PSlocalId</targetAttribute>
                    <sourceExpression>
                        <OCQL>pslocalid</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>PSnamespace</targetAttribute>
                    <sourceExpression>
                        <OCQL>psnamespace</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>containedBy</targetAttribute>
                    <sourceExpression>
                        <OCQL>datasetid</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>

        <FeatureTypeMapping>
            <mappingName>linkeddataset</mappingName>
            <sourceDataStore>dataStore</sourceDataStore>
            <sourceType>cdda_reporting_dataset</sourceType>
            <targetElement>cdda:linkedDataset</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>FEATURE_LINK[1]</targetAttribute>
                    <idExpression>
                        <OCQL>datasetid</OCQL>
                    </idExpression>
                    <sourceExpression>
                        <OCQL>cdda_reporting</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>datasetID</targetAttribute>
                    <sourceExpression>
                        <OCQL>datasetid</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>GmlFilename</targetAttribute>
                    <sourceExpression>
                        <OCQL>filename</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>WfsEndpoint</targetAttribute>
                    <sourceExpression>
                        <OCQL>wfsendpoint</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>WfsVersion</targetAttribute>
                    <sourceExpression>
                        <OCQL>wfsversion</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>WfsStoredQuery</targetAttribute>
                    <sourceExpression>
                        <OCQL>wfsstoredquery</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</appschema:AppSchemaDataAccess>
